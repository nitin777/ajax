<% title "Users" %>

<%= form_tag users_path, :method => 'get', :id => "users_search" do %>
  <p>
    Search: <%= text_field_tag :search, params[:search] %>
    <%#= submit_tag "Search", :name => nil %>
  </p>
  <div id="users"><%= render 'users' %></div>
<% end %>



<p><%= link_to "New user", new_user_path %></p>
